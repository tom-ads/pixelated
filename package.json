{
  "name": "pixelated",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:tom-ads/pixelated.git",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start:client": "yarn --cwd apps/client dev",
    "start:api": "yarn --cwd apps/server dev",
